<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.gob.cordoba.gobiernoabierto.portal-de-mapas" version="1.0.134" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Portal de mapas de la Ciudad de Córdoba</name>
    <description>
        Capas de datos geográficos de la Ciudad de Córdoba
    </description>
    <author email="gobiernoabiertocba@gmail.com" href="https://gobiernoabierto.cordoba.gob.ar/">
        Secretaría de Modernización de la Ciudad de Córdoba
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" src="www/res/icono-64x64.png" />
    <icon density="mdpi" src="www/res/icono-64x64.png" />
    <icon density="hdpi" src="www/res/icono-64x64.png" />
    <icon density="xhdpi" src="www/res/icono-64x64.png" />
    <splash src="www/res/splash.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
